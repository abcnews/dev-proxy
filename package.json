{
  "version": "1.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsup src/index.ts --dts --sourcemap --format esm,cjs --watch",
    "build": "tsup src/index.ts --dts --sourcemap --format esm,cjs",
    "test": "jest --coverage",
    "lint": "eslint src",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "size": "npm run build && size-limit",
    "analyze": "size-limit --why",
    "release": "np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@abcnews/dev-proxy",
  "author": "Simon Elvery",
  "module": "dist/index.mjs",
  "size-limit": [
    {
      "path": "dist/index.mjs",
      "limit": "1 KB"
    },
    {
      "path": "dist/index.js",
      "limit": "1.5 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "np": "^7.5.0",
    "size-limit": "^8.2.4",
    "ts-jest": "^29.1.0",
    "tslib": "^2.2.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
